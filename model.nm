    x : [0..4];

    [] x=0 -> 6/10:(x'=1) + 4/10:(x'=2);
    [] x=1 -> (x'=3);
    [] x=2 -> (x'=4);
    [] x=3 -> (x'=3);
    [] x=4 -> (x'=4);

